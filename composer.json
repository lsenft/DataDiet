{
    "name": "lsenft/data_diet",
    "description": "DataDiet plugin for CakePHP",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp"
    ],
    "homepage": "https://github.com/lsenft/data_diet",
    "license": "MIT",
    "authors": [
        {
            "name": "Levi Senft",
            "email": "lsenft@gmail.com",
            "homepage": "http://www.levisenft.com",
            "role": "Internets Tamer"
        }
    ],
    "support": {
        "issues": "https://github.com/lsenft/data_diet/issues",
        "source": "https://github.com/lsenft/data_diet"
    },
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.3.2 <4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Lsenft\\DataDiet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lsenft\\DataDiet\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
